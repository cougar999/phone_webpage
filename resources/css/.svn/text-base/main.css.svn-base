/*input */
input.checkbox{padding: 0;margin: 0;height: 16px;width: 16px;float: left;opacity: 0;}
label{ float: left; line-height: 16px; color: #fff; padding: 0 0 0 18px;-moz-transition: color 1s ease; -o-transition: color 1s ease; -webkit-transition: color 1s ease; transition: color 1s ease; }
input.checkbox:hover + label, input.checkbox:focus + label,
input.checkbox + label:hover, input.checkbox:focus + label,
input.checkbox:hover + label, input.checkbox:focus + label,
input.checkbox + label:hover, input.checkbox:focus + label			{ text-shadow: 1px 1px 3px #eee; color: #2C7AD0; }

input.checkbox + label{background: url(/resources/img40/gr_custom-inputs.png) 0 -1px no-repeat;height: 16px;}
#defapplist input.checkbox {float:none;}
#defapplist input.checkbox + label {margin:10px 0 0 5px;}

/* Checked styles */
input.checkbox[type=checkbox]:checked + label,input.checkbox[type=checkbox]:checked + label { background-position: 0 -81px; }
input.checkbox[type=checkbox]:hover:checked + label,
input.checkbox[type=checkbox]:focus:checked + label,
input.checkbox[type=checkbox]:checked + label:hover,
input.checkbox[type=checkbox]:focus:checked + label 					{ background-position: 0 -101px; }

/* Hover & Focus styles */
input.checkbox[type=checkbox]:hover + label,
input.checkbox[type=checkbox]:focus + label,
input.checkbox[type=checkbox] + label:hover 							{ background-position: 0 -21px; }

/* Active styles */
input.checkbox[type=checkbox]:active + label,
input.checkbox[type=checkbox] + label:hover:active						{ background-position: 0 -41px; }
input.checkbox[type=checkbox]:active:checked + label,
input.checkbox[type=checkbox]:checked + label:hover:active				{ background-position: 0 -121px; }

/* Disabled styles */
input.checkbox[type=checkbox]:disabled + label,
input.checkbox[type=checkbox]:hover:disabled + label,
input.checkbox[type=checkbox]:focus:disabled + label,
input.checkbox[type=checkbox]:disabled + label:hover,
input.checkbox[type=checkbox]:disabled + label:hover:active			{ background-position: 0 -61px; }
input.checkbox[type=checkbox]:disabled:checked + label,
input.checkbox[type=checkbox]:hover:disabled:checked + label,
input.checkbox[type=checkbox]:focus:disabled:checked + label,
input.checkbox[type=checkbox]:disabled:checked + label:hover,
input.checkbox[type=checkbox]:disabled:checked + label:hover:active	{ background-position: 0 -141px; }

