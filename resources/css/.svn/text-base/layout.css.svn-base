.fixed{table-layout:fixed;}
.auto{table-layout:auto;}

.loading{width:62px; height:62px; background:url(/resources/img/loading_black.gif) no-repeat center center;}
.loadings{position:fixed;top:40%;left:35%;width:300px;text-align:center;background:#fff;border:2px solid #efefef; border-radius:6px;box-shadow:1px 8px 5px rgba(68,68,68,0.2);padding:30px 30px 40px 30px;}
.loadsml{width:39px; height:39px; background:url(/resources/img/loading_black_small.gif) no-repeat center center;}

.mid {margin:20px auto;clear:both;width:980px;}

.btn{width:121px; height:30px; text-align:center; border:none; background:transparent url(/resources/img/index_bg.gif) no-repeat -2px -205px;cursor:pointer;}
button {width:70px; height:27px; line-height:27px; border:0; color:#333;cursor:pointer; background:url(/resources/img/btn_bg.jpg) no-repeat; font-size:12px;}
button:hover {background:url(/resources/img/btn_bg2.jpg) no-repeat;}
button.lbutton {width:auto; padding:0 5px; background:url(/resources/img/btn_bg2.gif) repeat-x 1px 0;border:1px solid #ccc;}
button.lbutton:hover {background:url(/resources/img/btn_bg3.gif) repeat-x 1px 0;border:1px solid #fde09a;}
button.disable,button.disable:hover { background:url(/resources/img/btn_bg2.gif) repeat-x;color:#ccc;border:1px solid #ccc;}

table{table-layout:fixed;}
table td {/*white-space:nowrap; */overflow:hidden; text-overflow:ellipsis;}
table.datatable tr:nth-of-type(odd),.datatable2 tr:nth-of-type(odd) td {background:#f4f9fc;}
table.datatable tr:nth-of-type(even),.datatable2 tr:nth-of-type(even) td {background:#fff;}
table.datatable tr:nth-of-type(odd):hover {background:#c2e0fb;}
table.datatable tr:nth-of-type(even):hover {background:#e9f5ff;}

/* new  datatable */
.datatable2 {width:100%;}
.datatable2 th {color:#4a4a4a;font-weight:normal;text-align:left;background:url(/resources/img40/gold_tabbg.png) repeat-x;font-size:12px;padding:3px 10px 4px;border-left:1px solid #c6cfd8;}
.datatable2 th.first {border-left:none;}
.datatable2 td {background:#fff;padding:5px 10px;font-size:12px;border-bottom:1px solid #f3f3f3;}
.datatable2 td a{color:#5898DA;}
.datatable2 tr:hover td {background:#e9f5ff;}

/* new ui 20120706*/
.unit {border:1px solid #e0e3e4;border-top-left-radius:5px;border-top-right-radius:5px;background:#FFF;margin-bottom:10px;}
.titlebar {height:28px;border-bottom:1px solid #e0e3e4;background:#fbfbfb;border-top-left-radius:5px;border-top-right-radius:5px;}
.titlebar h2 {font-size:12px;color:#555;line-height:28px;margin:0 20px 0 0;padding:0 0 0 10px;float:left;} 
.bread {float:left;margin-left:5px;}
.bread ul {padding:0;margin:0;}
.bread li {float:left;padding:0 12px;height:28px;line-height:26px;font-size:14px;color:#3e80da;list-style:none;background:url(/resources/img40/new-ui-spirit.png) no-repeat right top;}

/* webkit 滚动条 */
body::-webkit-scrollbar,
html::-webkit-scrollbar,
.scroll::-webkit-scrollbar,
.wrap::-webkit-scrollbar {
    width:16px;
	height:16px;
}
body::-webkit-scrollbar-track,
html::-webkit-scrollbar-track,
.scroll::-webkit-scrollbar-track,
.wrap::-webkit-scrollbar-track {/*滚动条的轨道*/
	background:#ecf5fc;
    /*box-shadow:inset 0 0 6px rgba(0,0,0,0.5);*/
}
body::-webkit-scrollbar-thumb,
html::-webkit-scrollbar-thumb,
.scroll::-webkit-scrollbar-thumb,
.wrap::-webkit-scrollbar-thumb {/*滚动条的滑块*/
    /*box-shadow:inset 0 0 6px rgba(0,0,0,0.5);*/
	background:#f1f6fa url(/resources/img40/scrollbar-arrow2.png) no-repeat center center;
	border:1px solid #bccee8;
	border-radius:3px;
}
body::-webkit-scrollbar-thumb:hover,
html::-webkit-scrollbar-thumb:hover,
.scroll::-webkit-scrollbar-thumb:hover,
.wrap::-webkit-scrollbar-thumb:hover {/*滚动条的悬浮状态*/
	background:#cdeaf9 url(/resources/img40/scrollbar-arrow2.png) no-repeat center center; 
	border:1px solid #bccee8; 
   /* box-shadow:inset 0 6px 0 rgba(208,230,251,0.9);*/
}
body::-webkit-scrollbar-thumb:active,
html::-webkit-scrollbar-thumb:active,
.scroll::-webkit-scrollbar-thumb:active,
.wrap::-webkit-scrollbar-thumb:active {/*滚动条的悬浮状态*/
	background:#b2d8ee url(/resources/img40/scrollbar-arrow3.png) no-repeat center center; 
	border:1px solid #bccee8; 
   /* box-shadow:inset 0 6px 0 rgba(208,230,251,0.9);*/
}
body::-webkit-scrollbar-thumb:window-inactive,
html::-webkit-scrollbar-thumb:window-inactive,
.scroll::-webkit-scrollbar-thumb:window-inactive,
.wrap::-webkit-scrollbar-thumb:window-inactive {/*焦点不在当前窗口的时候*/
	background:#d0e6fb; 
}
body::-webkit-scrollbar-button:vertical:decrement,
html::-webkit-scrollbar-button:vertical:decrement,
.scroll::-webkit-scrollbar-button:vertical:decrement,
.wrap::-webkit-scrollbar-button:vertical:decrement {
	height:16px;
	width:16px;
	background:url(/resources/img40/scrollbar-arrow.png) no-repeat center top;
}
body::-webkit-scrollbar-button:vertical:increment,
html::-webkit-scrollbar-button:vertical:increment,
.scroll::-webkit-scrollbar-button:vertical:increment,
.wrap::-webkit-scrollbar-button:vertical:increment {
	height:16px;
	width:16px;
	background:url(/resources/img40/scrollbar-arrow.png) no-repeat center -17px;
}
body::-webkit-scrollbar-button:horizontal:decrement,
html::-webkit-scrollbar-button:horizontal:decrement,
.scroll::-webkit-scrollbar-button:horizontal:decrement,
.wrap::-webkit-scrollbar-button:horizontal:decrement {
	height:16px;
	width:16px;
	background:url(/resources/img40/scrollbar-arrow.png) no-repeat center -32px;
}
body::-webkit-scrollbar-button:horizontal:increment,
html::-webkit-scrollbar-button:horizontal:increment,
.scroll::-webkit-scrollbar-button:horizontal:increment,
.wrap::-webkit-scrollbar-button:horizontal:increment {
	height:16px;
	width:16px;
	background:url(/resources/img40/scrollbar-arrow.png) no-repeat center -48px;
}
