html,body{background:#FFF; height:100%; border:0px; margin:0px; padding:0px;-webkit-text-size-adjust:none;}
body{height:100%;font-size:14px;color:#3c3c3c; font-family:'Microsoft Yahei','Segoe UI','Lucida Grande',Verdana,Arial,Helvetica,sans-serif,'Simsun'; line-height:130%;padding:0;margin:0;border:0;}
table,td,th{ font-family:'Microsoft Yahei','Segoe UI','Lucida Grande',Verdana,Arial,Helvetica,sans-serif,'Simsun';font-size:14px;}
input{font-family:'Microsoft Yahei';border-radius: 3px 3px 3px 3px; border: 1px solid #ccc;display: inline-block;
				-moz-transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
				-webkit-transition: border 0.2s linear 0s, box-shadow 0.2s linear 0s;
				box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;padding: 4px;}
input:focus,textarea:focus {border-color: rgba(82, 168, 236, 0.8);
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(82, 168, 236, 0.6);
    outline: 0 none;
	background:#fefef3;}
input[type=image] {border:none;box-shadow:none;}
h1{margin:0.5em 0;}
h2{font-size:16px;color:#777d96;}
h3{margin:0.5em 0;}
a{text-decoration:none;color:#333;outline:none;}
a:hover{color:#3181eb;transition:color .25s linear;-webkit-transition:color .25s linear;}
img {border:none;}
.page,.body,.wrap{margin:0; padding:0; width:100%; height:100%;}

.head,.foot{margin:0; padding:0; width:100%; height:0px;}

.container{margin:0; padding:0; width:100%; height:100%;}
.container .left{float:left; width:184px; height:100%; background:#ededf5 url(/resources/img40/leftbarbg.jpg?d=20120614101726) repeat-y right top;}
.container .right{float:right; width:0px; height:100%;}
.container .center{ margin:0 0 0 184px; height:100%;}

.content{ margin:0; padding:0; height:100%;}
.content .left{float:left; width:0px; height:100%;}
.content .right{float:right; width:220px; height:100%;margin-top:5px; margin-right:15px;}
.content .center{margin:0 235px 0 0; height:100%;padding: 5px 10px 5px 15px;}

.contentside{margin:0; padding:0 0 0 1px; height:42px; overflow-y:hidden;background:url(/resources/img40/toolbar_side.png?d=20120614101726) no-repeat left top;}
.contentbody{margin:0; padding:0;}
.contentwrap{margin:0; padding:10px;}

.shadow {
	-moz-box-shadow: 2px 2px 2px rgba(68,68,68,0.3);
	-webkit-box-shadow: 2px 2px 2px rgba(68,68,68,0.3);
	/* For IE 5.5 - 7 */
	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=2, Direction=135, Color='#444444');
	/* For IE 8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=2, Direction=135, Color='#444444')";
	box-shadow: 2px 2px 2px rgba(68,68,68,0.3);
}

table.fixwidth{table-layout:fixed;}
td.fixwidth{white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.scroll {overflow-y:auto;overflow-x:auto;}
/*scroll的绝对定位是为了让滚动条不被上面的toolbar遮住*/
.scrollbox {margin:0; padding:0;}

